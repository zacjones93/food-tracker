{
  "name": "cloudflare-workers-nextjs-saas-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:prod": "pnpm run opennext:build && wrangler deploy --dry-run --outdir=dist",
    "opennext:build": "opennextjs-cloudflare build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "pnpm run opennext:build && opennextjs-cloudflare deploy",
    "preview": "pnpm run opennext:build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate --name",
    "db:migrate:local": "wrangler d1 migrations apply $(node scripts/get-db-name.mjs) --local",
    "db:migrate:remote": "wrangler d1 migrations apply $(node scripts/get-db-name.mjs) --remote",
    "db:seed:recipes:local": "wrangler d1 execute $(node scripts/get-db-name.mjs) --file=src/db/seed.sql --local",
    "db:seed:recipes:remote": "wrangler d1 execute $(node scripts/get-db-name.mjs) --file=src/db/seed.sql --remote",
    "db:seed:weeks:local": "wrangler d1 execute $(node scripts/get-db-name.mjs) --file=src/db/weeks-seed.sql --local",
    "db:seed:weeks:remote": "wrangler d1 execute $(node scripts/get-db-name.mjs) --file=src/db/weeks-seed.sql --remote",
    "db:seed:template:local": "wrangler d1 execute $(node scripts/get-db-name.mjs) --file=src/db/grocery-template-seed.sql --local",
    "db:seed:template:remote": "wrangler d1 execute $(node scripts/get-db-name.mjs) --file=src/db/grocery-template-seed.sql --remote",
    "db:seed:team:local": "wrangler d1 execute $(node scripts/get-db-name.mjs) --file=scripts/seed-default-team.sql --local",
    "db:seed:team:remote": "wrangler d1 execute $(node scripts/get-db-name.mjs) --file=scripts/seed-default-team.sql --remote",
    "db:seed:ingredients:local": "wrangler d1 execute $(node scripts/get-db-name.mjs) --file=src/db/update-ingredients.sql --local",
    "db:seed:ingredients:remote": "wrangler d1 execute $(node scripts/get-db-name.mjs) --file=src/db/update-ingredients.sql --remote",
    "db:seed:local": "pnpm db:seed:team:local && pnpm db:seed:recipes:local && pnpm db:seed:weeks:local && pnpm db:seed:template:local && pnpm db:seed:ingredients:local",
    "db:seed:remote": "pnpm db:seed:team:remote && pnpm db:seed:recipes:remote && pnpm db:seed:weeks:remote && pnpm db:seed:template:remote && pnpm db:seed:ingredients:remote",
    "db:clear:local": "wrangler d1 execute $(node scripts/get-db-name.mjs) --local --command \"DELETE FROM grocery_items; DELETE FROM week_recipes; DELETE FROM recipe_relations; DELETE FROM weeks; DELETE FROM recipes; DELETE FROM recipe_books; DELETE FROM grocery_list_templates; DELETE FROM team_invitation; DELETE FROM team_membership; DELETE FROM team_role; DELETE FROM team;\"",
    "db:clear:remote": "wrangler d1 execute $(node scripts/get-db-name.mjs) --remote --command \"DELETE FROM grocery_items; DELETE FROM week_recipes; DELETE FROM recipe_relations; DELETE FROM weeks; DELETE FROM recipes; DELETE FROM recipe_books; DELETE FROM grocery_list_templates; DELETE FROM team_invitation; DELETE FROM team_membership; DELETE FROM team_role; DELETE FROM team;\"",
    "db:reseed:local": "pnpm db:clear:local && pnpm db:seed:local",
    "db:reseed:remote": "pnpm db:clear:remote && pnpm db:seed:remote",
    "d1:cache:clean": "wrangler d1 execute $(node scripts/get-db-name.mjs) --command \"DELETE FROM tags; DELETE FROM revalidations\" --remote",
    "email:dev": "email dev -d src/react-email -p 3001",
    "list:kv": "wrangler kv key list --namespace-id=$(node scripts/get-kv-id.mjs) --remote | jq '[.[].name]' > kv.log",
    "delete:kv": "wrangler kv bulk delete kv.log --namespace-id=$(node scripts/get-kv-id.mjs) --remote",
    "scrape:all": "pnpm scrape:recipes && pnpm scrape:schedule",
    "scrape:recipes": "tsx scripts/scrape-notion-recipes.ts",
    "scrape:recipes:full": "pnpm scrape:recipes && pnpm parse:ingredients",
    "scrape:schedule": "tsx scripts/scrape-notion-schedule.ts",
    "scrape:relations": "tsx scripts/generate-week-recipe-relations.ts",
    "parse:ingredients": "tsx scripts/parse-ingredients-parallel.ts",
    "seed:grocery-template": "tsx scripts/seed-default-grocery-template.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@heroicons/react": "^2.2.0",
    "@heroui/react": "^2.7.6",
    "@hookform/resolvers": "^3.10.0",
    "@icons-pack/react-simple-icons": "^11.2.0",
    "@marsidev/react-turnstile": "^1.1.0",
    "@notionhq/client": "^5.2.1",
    "@opennextjs/cloudflare": "^1.11.0",
    "@oslojs/encoding": "^1.1.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-email/components": "0.0.32",
    "@react-email/render": "1.0.4",
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/server": "^13.1.1",
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^6.0.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/bcryptjs": "^3.0.0",
    "arctic": "^3.1.3",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.6",
    "ipaddr.js": "^2.2.0",
    "lucide-react": "^0.471.0",
    "motion": "^11.18.0",
    "ms": "^2.1.3",
    "next": "15.4.6",
    "next-themes": "^0.4.6",
    "nextjs-toploader": "^3.8.16",
    "notion-to-md": "^3.1.9",
    "nuqs": "^2.7.2",
    "phosphor-react": "^1.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "react-markdown": "^10.1.0",
    "react-turnstile": "^1.1.4",
    "remeda": "^2.22.1",
    "slugify": "^1.6.6",
    "sonner": "^1.7.1",
    "stripe": "^17.7.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "ua-parser-js": "^2.0.0",
    "usehooks-ts": "^3.1.0",
    "zod": "^3.25.28",
    "zsa": "^0.6.0",
    "zsa-react": "^0.2.3",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.5",
    "@simplewebauthn/types": "^12.0.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.19.21",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "better-sqlite3": "^12.4.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.5",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "postcss": "^8",
    "react-email": "3.0.6",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "wrangler": "^4.43.0"
  }
}
